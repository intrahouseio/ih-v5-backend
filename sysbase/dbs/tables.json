  },

  "scene": {
    "store": "db",
    "collection": "scenes",
    "ruleID": { "pref": "scen", "len": 3 }
  },
  "scenesTreeTable": { "store": "tree", "tree": "scenes", "rows": true },

  "scenecall": {
    "store": "db",
    "collection": "scenecalls",
    "rows": true,
    "ruleID": { "pref": "call", "len": 3 }
  },

  "handlergroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "handlergroup" },
    "defRootTitle": "Функции-обработчики",
    "default": { "name": "$NewFolde" },
    "ruleID": { "pref": "fng", "len": 3 }
  },

  "handler": {
    "store": "db",
    "collection": "handlers",
    "ruleID": { "pref": "fun", "len": 3 }
  },


  "plugingroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "plugingroup" },
    "defRootTitle": "Плагины",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "pg", "len": 3 }
  },

  "unitgroup": {
    "store": "db",
    "collection": "units",
    "filter": { "folder": 1 },
    "defRootTitle": "Плагины",
    "ruleID": { "pref": "pg", "len": 3 }
  },

  "units": {
    "store": "db",
    "collection": "units",
    "default": { "name": "Новый плагин" },
    "ruleID": { "pref": "u", "len": 3 }
  },

  "snippetgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "snippetroup" },
    "defRootTitle": "Сниппеты",
