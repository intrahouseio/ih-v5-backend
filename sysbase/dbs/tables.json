{
  "place": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "place" },
    "defRootTitle": "Все",
    "default": { "name": "$NewFolder" },
    "ruleID":{"pref":"p", "len":4}
  },

  "device": { "store": "db", "collection": "devices", "default": { "name": "$NewDevice" } },
  "devprops": { "store": "db", "collection": "devprops", "genfield": "aux", "default": { "aux": {} } },
  "devhard": { "store": "db", "collection": "devhard" },
  "devcurrent": { "store": "db", "collection": "devcurrent" },
  "devicecommonTable": { "store": "db", "collection": "devprops", "genfield": "aux" },
  "typegroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "typegroup" },
    "defRootTitle": "Типы",
    "default": { "name": "$NewFolder" }
  },

  "type": {
    "store": "db",
    "collection": "types",
    "genfield": "props",

    "validator": {
      "main": {
        "name": { "required": true, "empty": false, "description": "$mustBeNotEmptyString" }
      },
      "props": {
        "name": { "required": true, "type": "string", "empty": false, "description": "$mustBeNotEmptyString" },
        "vtype": { "required": true, "type": "string", "empty": false, "description": "$mustBeNotEmptyString" },
        "op": { "required": true, "type": "string", "empty": false, "description": "$mustBeNotEmptyString" },
        "min": { "type": "number", "description": "$mustBeLessToMax", "spec": "minmax" },
        "max": { "type": "number", "description": "$mustBeGreaterToMin", "spec": "minmax" }
      }
    },

    "default": { "name": "$NewType", "props": { "value": { "name": "Значение", "vtype": "B", "op": "r" } } }
  },

  "layoutgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "layoutgroup" },
    "defRootTitle": "Экраны",
    "default": { "name": "$NewFolder" }
  },
  "layout": { "store": "db", "collection": "layouts", "default": { "name": "Новый экран" } },
  "scriptgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "scriptgroup" },
    "defRootTitle": "Сценарии",
    "default": { "name": "$NewFolder" }
  },
  "script": { "store": "db", "collection": "scripts", "default": { "name": "Новый сценарий" } },

  "plugingroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "plugingroup" },
    "defRootTitle": "Плагины",
    "default": { "name": "$NewFolder" }
  },

  "snippetgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "snippetroup" },
    "defRootTitle": "Сниппеты",
    "default": { "name": "$NewFolder" }
  },

  "units": { "store": "db", "collection": "units", "default": { "name": "Новый плагин" } },
  "snippets": { "store": "db", "collection": "snippets", "default": { "name": "Новый сниппет" } },

  "chartgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "chartgroup" },
    "defRootTitle": "Графики",
    "default": { "name": "$NewFolder" }
  },

  "reportgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "reportgroup" },
    "defRootTitle": "Отчеты",
    "default": { "name": "$NewFolder" }
  },

  "charts": { "store": "db", "collection": "charts", "genfield": "props", "default": { "name": "Новый график" } },

  "reports": { "store": "db", "collection": "reports", "genfield": "props", "default": { "name": "Новый отчет" } },

  "user": { "store": "db", "collection": "users" },
  "token": { "store": "db", "collection": "tokens" }
}
