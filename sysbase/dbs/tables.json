      "props": {
        "newid": { "notempty": true }
      }
    }
  },

  "devicehwTable": {
    "store": "db",
    "collection": "devices",
    "genfield": "props",
    "rows": true
  },

  "unitchannelsTable": { "store": "db", "collection": "devhard", "rows": true },
