{
  "place": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "place" },
    "defRootTitle": "Все",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "dg", "len": 3 }
  },

  "device": {
    "store": "db",
    "collection": "devices",
    "ruleID": { "pref": "d", "len": 4 },
    "validator": {
      "main": {
        "name": { "required": true, "notempty": true },
        "dn": { "required": true, "notempty": true, "unique": true },
        "type": { "required": true, "notempty": true }
      }
    }
  },

  "globals": {
    "store": "db",
    "collection": "globals",
    "ruleID": { "pref": "gl", "len": 3 },
    "validator": {
      "main": {
        "name": { "required": true, "notempty": true },
        "dn": { "required": true, "notempty": true, "unique": true }
      }
    }
  },

  "globalgroup": {
    "store": "db",
    "collection": "globals",
    "filter": { "folder": 1 },
    "defRootTitle": "Глобальные переменные",
    "default": { "name": "New Folder" },
    "ruleID": { "pref": "glg", "len": 3 }
  },

  "locals": {
    "store": "db",
    "collection": "locals",
    "ruleID": { "pref": "local", "len": 3 },
    "validator": {
      "main": {
        "name": { "required": true, "notempty": true },
        "dn": { "required": true, "notempty": true, "unique": true }
      }
    }
  },

  "localgroup": {
    "store": "db",
