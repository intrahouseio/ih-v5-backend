  },

  "chartgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "chartgroup" },
    "defRootTitle": "Графики",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "cg", "len": 3 }
  },

  "reportgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "reportgroup" },
    "defRootTitle": "Отчеты",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "rg", "len": 3 }
  },

  "charts": {
    "store": "db",
    "collection": "charts",
    "genfield": "props",
    "default": { "name": "Новый график" },
    "ruleID": { "pref": "c", "len": 3 }
  },

  "reports": {
    "store": "db",
    "collection": "reports",
    "genfield": "props",
    "default": { "name": "Новый отчет" },
    "ruleID": { "pref": "r", "len": 3 }
  },

  "usergroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "usergroup" },
    "defRootTitle": "Пользователи",
    "default": { "name": "New User" },
    "ruleID": { "pref": "ug", "len": 3 }
  },

  "user": { "store": "db", "collection": "users" },
  "token": { "store": "db", "collection": "tokens" }
}
