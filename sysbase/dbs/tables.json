  "typegroup": {
    "store": "db",
    "collection": "types",
    "filter": { "folder": 1 },
    "defRootTitle": "Типы устройств",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "tg", "len": 3 }
  },

  "type": {
    "store": "db",
    "collection": "types",
    "genfield": "props",

    "validator": {
      "main": {
        "name": { "notempty": true }
      },
      "props": {
        "newid": { "notempty": true, "validid": true },
        "name": { "notempty": true },
        "op": { "notempty": true },
        "min": { "type": "number", "spec": "min" },
        "max": { "type": "number", "spec": "max" }
      }
    },

    "default": {
      "name": "$NewType",
      "props": { "value": { "name": "Значение", "vtype": "B", "op": "rw", "fuse": 1 } }
    },
    "ruleID": { "pref": "t", "len": 3 }
  },

  "typepropsTable": {
    "store": "db",
    "collection": "types",
    "genfield": "props",

    "validator": {
      "main": {
        "name": { "notempty": true }
      },
      "props": {
        "newid": { "notempty": true, "validid": true },
        "name": { "notempty": true },
        "op": { "notempty": true },
        "min": { "type": "number", "spec": "min" },
        "max": { "type": "number", "spec": "max" }
      }
    },

    "default": { "name": "$NewType", "props": { "value": { "name": "Значение", "vtype": "B", "op": "r" } } },
    "ruleID": { "pref": "t", "len": 3 }
  },

  "typesTreeTable": { "store": "tree", "tree": "types", "collection": "types", "rows": true },
  "typedevicesTable": { "store": "db", "collection": "devices", "rows": true },

