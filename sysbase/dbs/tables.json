{
  "place": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "place" },
    "defRootTitle": "Все",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "dg", "len": 3 }
  },

  "device": {
    "store": "db",
    "collection": "devices",
    "ruleID": { "pref": "d", "len": 4 },
    "validator": {
      "main": {
        "name": { "required": true, "notempty": true },
        "dn": { "required": true, "notempty": true, "unique": true }
      }
    }
  },

  "chgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "chgroup" },
    "defRootTitle": "Каналы",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "chgr", "len": 3 }
  },
  "channels": { "store": "db", "collection": "devhard" },

  "devhard": { "store": "db", "collection": "devhard" },
  "devcurrent": { "store": "db", "collection": "devcurrent" },

  "devicesTreeTable": { "store": "tree", "tree": "devdevices", "rows": true },

  "devicecommonTable": {
    "store": "db",
    "collection": "devices",
    "genfield": "props",
    "validator": {
      "props": {
        "newid": { "notempty": true, "validid": true },
        "min": { "type": "number", "spec": "min" },
        "max": { "type": "number", "spec": "max" }
      }
    }
  },

  "deviceprops": {
    "store": "db",
    "collection": "devices",
    "genfield": "props"
  },
  

  "devicedbTable": {
    "store": "db",
    "collection": "devicedb",
    "genfield": "props",
    "validator": {
      "props": {
        "newid": { "notempty": true }
      }
    }
  },

  "devicehwTable": {
    "store": "db",
    "collection": "devices",
    "genfield": "props",
    "rows": true
    
  },
  
  "unitchannelsTable": { "store": "db", "collection": "devhard", "rows": true },
  "typegroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "typegroup" },
    "defRootTitle": "Типы",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "tg", "len": 3 }
  },

  "type": {
    "store": "db",
    "collection": "types",
    "genfield": "props",

    "validator": {
      "main": {
        "name": { "notempty": true }
      },
      "props": {
        "newid": { "notempty": true, "validid": true },
        "name": { "notempty": true },
        "vtype": { "notempty": true },
        "op": { "notempty": true },
        "min": { "type": "number", "spec": "min" },
        "max": { "type": "number", "spec": "max" }
      }
    },

    "default": { "name": "$NewType", "props": { "value": { "name": "Значение", "vtype": "B", "op": "r" } } },
    "ruleID": { "pref": "t", "len": 3 }
  },
  "typesTreeTable": { "store": "tree", "tree": "types", "collection": "types", "rows": true },
  "typedevicesTable": { "store": "db", "collection": "devices", "rows": true },

  "layoutgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "layoutgroup" },
    "defRootTitle": "Экраны",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "lg", "len": 3 }
  },

  "layout": {
    "store": "db",
    "collection": "layouts",
    "default": { "name": "Новый экран" },
    "ruleID": { "pref": "l", "len": 3 }
  },

 "viscontgroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "viscontgroup" },
    "defRootTitle": "Контейнеры",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "vcg", "len": 3 }
  },

  "viscont": {
    "store": "db",
    "collection": "visconts",
    "default": { "name": "Новый контейнер" },
    "ruleID": { "pref": "vc", "len": 3 }
  },

  "vistemplategroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "vistemplategroup" },
    "defRootTitle": "Шаблоны",
    "default": { "name": "$NewFolder" },
    "ruleID": { "pref": "vtg", "len": 3 }
  },

  "vistemplate": {
    "store": "db",
    "collection": "vistemplates",
    "default": { "name": "Новый шаблон" },
    "ruleID": { "pref": "vt", "len": 3 }
  },
  

  "scenegroup": {
    "store": "db",
    "collection": "lists",
    "filter": { "list": "scenegroup" },
    "defRootTitle": "Сценарии",
    "default": { "name": "$NewFolder" },
