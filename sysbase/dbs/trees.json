{
  "dev": ["devdevices", "sysdevices"],

  "devdevices": {
    "branch": { "table": "place", 
      "propmap": { "_id": "id", "name": "title", "parent": "parent", "order": "order" } },
    "leaf": {
      "table": "device",
      "propmap": { "_id": "id", "name": "name", "title": "title", "parent": "parent", "order": "order", "dn": "dn" }
    }
  },

  "sysdevices": {
    "branch": {
      "table": "sysdevicegroupTable",
      "propmap": { "_id": "id", "name": "title", "parent": "parent", "order": "order" }
    },
    "leaf": { "table": "sysdeviceTable", "propmap": { "_id": "id", "name": "title", "parent": "parent", "order": "order" } }
  },

  "types": {
    "branch": {
      "table": "typegroup",
      "propmap": { "_id": "id", "name": "title", "parent": "parent", "order": "order" }
