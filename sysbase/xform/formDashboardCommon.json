{
  "grid": [
    { "id": "p1", "xs": 3, "class": "main" },
    { "id": "p2", "xs": 3, "class": "main" },
    { "id": "p3", "xs": 3, "class": "main" },
    { "id": "p4", "xs": 3, "class": "main" },
    { "id": "p9", "xs": 12, "class": "main", "hide": "data.p3.conf<2" }
  ],
  "spacing": 10,

  "p1": [
    { "prop": "sysinfo", "title": "$SystemInfoName", "type": "header", "size": "h6" },
    { "prop": "platform", "title": "$process_platform", "type": "text" },
    { "prop": "arch", "title": "$process_arch", "type": "text" }
  ],
  "p2": [
    { "prop": "processinfo", "title": "$MemoryInfoName", "type": "header", "size": "h6" },
    { "prop": "osMemory", "title": "$os_memory", "type": "text" },
    { "prop": "rss", "title": "$os_memory_rss", "type": "text" },
    { "prop": "heapTotal", "title": "$os_memory_heap_total", "type": "text" },
    { "prop": "heapUsed", "title": "$os_memory_heap_used", "type": "text" }
  ],
  "p3": [
    { "prop": "projectinfo", "title": "$ProjectInfoName", "type": "header", "size": "h6" },
    { "prop": "project", "title": "$Project_Folder", "type": "text" },
    { "prop": "type", "title": "$qTypes", "type": "text" },
    { "prop": "device", "title": "$qDevices", "type": "text" },
    { "prop": "devhard", "title": "$qChannels", "type": "text" },
    { "prop": "conf", "title": "", "type": "text", "hide": "1==1" }
  ],
  "p4": [
    { "prop": "uptimeinfo", "title": "$Version&Uptime", "type": "header", "size": "h6" },
    { "prop": "osUptime", "title": "$os_uptime", "type": "text" },
    { "prop": "uptime", "title": "$process_uptime", "type": "text" },
    { "prop": "version", "title": "$Version", "type": "text" },
    { "prop": "restartButton", "title": "$RestartIH", "type": "button", "command": "restart", "param": "" },
    { "prop": "newversion", "title": "$NEWVERSION", "type": "text", "hide": "!data.p4.newversion" },
    {
      "prop": "checkButton",
      "title": "$CHECKUPDATE",
      "type": "button",
      "command": "checkupdate",
      "param": "",
      "hide": "data.p4.newversion"
    },
    {
      "prop": "updateButton",
      "title": "$UPDATE",
      "type": "button",
      "command": "update",
      "param": "",
      "hide": "!data.p4.newversion"
    }
  ],

  "p9": [
    {
      "prop": "block_header",
      "title": "Число привязанных каналов превышает доступные! Исполнительная система переведена в режим ограниченного функционала!",
      "type": "header",
      "size": "h6",
      "hide":"!data.p9.limitLht"
    },
    { "prop": "limitLht", "title": "", "type": "text", "hide":"1==1" },
    { "prop": "usedLht", "title": "$qTags", "type": "text" },
    { "prop": "enabledLht", "title": "Лицензировано", "type": "text" }
  ]
}
