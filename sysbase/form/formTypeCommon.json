{
  "grid": [
    { "id": "p1", "xs": 6, "class": "main", "table": "type" },
    { "id": "p2", "xs": 6, "class": "main", "table": "type" },
    { "id": "p3", "xs": 12, "class": "main", "table": "type", "height": 250 }
  ],
  "spacing": 10,
  "breadcrumbs": { "tree": "types" },

  "p1": [
    { "prop": "name", "title": "$Name", "type": "input" },
    { "prop": "ruledn_pref", "title": "$Ruledn_pref", "type": "input"},
    { "prop": "tags", "title": "Tags", "type": "tags", "data": "taglist" }
   
  ],
  "p2": [{ "prop": "txt", "title": "$Comment", "type": "textarea" }],
  "p3": [
    {
      "title": "$DeviceProperty",
      "type": "table",
      "popupenable":true,
      "prop": "typepropsTable",

      "columns": [
    
        { "prop": "newid", "title": "$Devprop_Id", "type": "input", "width": 120 },
        { "prop": "name", "title": "$Name", "type": "input", "width": 200 },
        {
          "prop": "vtype",
          "title": "$Devprop_Type",
          "type": "droplist",
          "data": [
            { "id": "B", "title": "Boolean" },
            { "id": "N", "title": "Number" },
            { "id": "S", "title": "String" }
          ],
          "width": 100
        },
        {
          "prop": "op",
          "title": "$Operations",
          "type": "droplist",
          "data": [
            { "id": "r", "title": "Read only" },
            { "id": "rw", "title": "Read/Write" },
            { "id": "w", "title": "Write only" },
            { "id": "c", "title": "Calculate" }
          ],
          "width": 100
        },
        { "prop": "min", "title": "Min", "type": "number", "width": 50 },
        { "prop": "max", "title": "Max", "type": "number", "width": 50 },
        { "prop": "dig", "title": "$Decdig", "type": "number", "width": 100 },
        { "prop": "mu", "title": "$Dev_Mu", "type": "input", "width": 50 },
        { "prop": "onread", "title": "При получении данных", "type": "droplist", "data":"readhandlers", "width": 200 },
        { "prop": "onwrite", "title": "При отправке данных", "type": "droplist", "data":"writehandlers", "width": 200 }

      ]
    }
  ]
}
