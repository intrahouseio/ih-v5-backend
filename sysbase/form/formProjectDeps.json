{
  "grid": [
    { "id": "p2", "xs": 12, "class": "main", "table": "jlevelsTable", "height": 300 },
    { "id": "p3", "xs": 6, "class": "main", "table": "project" },

    { "id": "p5", "xs": 6, "class": "main", "table": "projectdepsTable", "height": 300, "hide": "!data.p3.deps" }
  ],
  "spacing": 10,
  "breadcrumbs": { "tree": "projects" },

  "p2": [
 

    { "prop": "jlevel_txt", "title": "Уровни сообщений в журналах", "type": "header" },
    {
      "type": "table",
      "prop": "jlevels",
      "title": "",
   
      "popupenable": true,
   

      "columns": [
        { "prop": "level", "title": "Уровень сообщения", "type": "number", "width": 150 },
        { "prop": "name", "title": "Описание", "type": "input", "width": 250 },
        { "prop": "levelcolor", "title": "$Color", "type": "color", "width": 50 },
        { "prop": "days", "title": "Срок хранения", "type": "number", "width": 150 }
      ]
    }
  ],

  "p3": [
    { "prop": "deps", "title": "Deps", "type": "cb", "hide": "1==1" },
    { "prop": "__expert", "title": "", "type": "text", "hide": "1==1" },
   

    { "prop": "nodeps_txt", "title": "Проект не имеет внешних модулей", "type": "header", "hide": "data.p3.deps" },
    {
      "prop": "deps_txt",
      "title": "Для установки npm модуля введите имя пакета",
      "type": "header",
      "hide": "!data.p3.deps || !data.p3.__expert"
    },
    { "prop": "newmodule", "title": "", "type": "input", "hide": "!data.p3.deps || !data.p3.__expert" }
  ],
  "p5": [
    { "prop": "deps_txt", "title": "Установлены npm модули", "type": "header" },
    {
      "type": "table",
      "prop": "projectdeps",

      "columns": [
        { "prop": "name", "title": "NPM module", "type": "text", "width": 150 },

        { "prop": "version", "title": "$Version", "type": "text", "width": 150 },
        { "prop": "from", "title": "From", "type": "text", "width": 150 }
      ]
    }
  ]
}
