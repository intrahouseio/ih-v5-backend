{
  "grid": [
    { "id": "p1", "xs": 6, "class": "main", "table": "project" },

    { "id": "p2", "xs": 6, "class": "main", "table": "projectdepsTable", "height": 300, "hide": "!data.p1.deps" }
  ],
  "spacing": 10,
  "breadcrumbs": { "tree": "projects" },

  "p1": [
    { "prop": "deps", "title": "Deps", "type": "cb", "hide": "1==1" },
    { "prop": "__expert", "title": "", "type": "text", "hide": "1==1" },
   

    { "prop": "nodeps_txt", "title": "Проект не имеет внешних модулей", "type": "header", "hide": "data.p1.deps" },
    {
      "prop": "deps_txt",
      "title": "Для установки npm модуля введите имя пакета",
      "type": "header",
      "hide": "!data.p1.deps || !data.p1.__expert"
    },
    { "prop": "newmodule", "title": "", "type": "input", "hide": "!data.p1.deps || !data.p1.__expert" }
  ],
  "p2": [
    { "prop": "deps_txt", "title": "Установлены npm модули", "type": "header" },
    {
      "type": "table",
      "prop": "projectdeps",

      "columns": [
        { "prop": "name", "title": "NPM module", "type": "text", "width": 150 },

        { "prop": "version", "title": "$Version", "type": "text", "width": 150 },
        { "prop": "from", "title": "From", "type": "text", "width": 150 }
      ]
    }
  ]
}
