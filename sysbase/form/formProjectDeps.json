{
  "grid": [
    { "id": "p1", "xs": 12, "class": "main", "table": "project" },
    { "id": "p2", "xs": 12, "class": "main", "table": "projectdepsTable", "height": "fill", "hide": "!data.p1.deps" }
  ],
  "spacing": 10,
  "breadcrumbs": { "tree": "projects" },

  "p1": [
    { "prop": "title", "title": "$Project_Name", "type": "text" },
    { "prop": "deps", "title": "Deps", "type": "cb", "hide": "1==1" },
    { "prop": "nodeps_txt", "title": "Проект не имеет внешних зависимостей", "type": "header", "hide": "data.p1.deps" },
    { "prop": "newmodule", "title": "Установить модуль", "type": "input", "hide": "data.p1.deps" }
  ],
  "p2": [
    { "prop": "deps_txt", "title": "Установлены модули", "type": "header" },
    {
      "type": "table",
      "prop": "projectdeps",

      "columns": [
        { "prop": "name", "title": "NPM module", "type": "text", "width": 250 },

        { "prop": "version", "title": "$Version", "type": "text", "width": 250 },
        { "prop": "from", "title": "From", "type": "text", "width": 250 }
      ]
    }
  ]
}
