{
  "grid": [
    { "id": "p1", "xs": 6, "class": "main", "table": "projects" },
    { "id": "p2", "xs": 6, "class": "main", "table": "projects" }
  ],
  "spacing": 10,
  "breadcrumbs": { "tree": "projects" },

  "p1": [
    { "prop": "_id", "title": "ID ", "type": "text" },
    { "prop": "projectfolder", "title": "$Project_Folder", "type": "input", "hide":"data.p1._id == data.p1.folder" },
    { "prop": "title", "title": "$Project_Name", "type": "input" },
    { "prop": "version", "title": "$Project_Version", "type": "text" },
    { "prop": "activeButton", "title": "Activate project", "type": "button", "command": "restart" }

  ],
  "p2": [{ "prop": "txt", "title": "$Comment", "type": "textarea" }]
  
}